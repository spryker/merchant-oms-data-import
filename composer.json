{
  "name": "spryker/merchant-oms-data-import",
  "type": "library",
  "description": "MerchantOmsDataImport module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/data-import": "^1.0.0",
    "spryker/kernel": "^3.30.0",
    "spryker/merchant": "^2.2.0",
    "spryker/merchant-oms": "^1.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
